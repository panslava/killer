{
  "title": "User",
  "type": "object",
  "properties": {
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "name": {
      "title": "name",
      "type": "object",
      "properties": {
        "first": {
          "type": "string"
        },
        "last": {
          "type": "string"
        }
      }
    },
    "course": {
      "type": "number",
      "minimum": 1,
      "maximum": 8
    },
    "photo": {
      "type": "string"
    },
    "vk": {
      "type": "string"
    },
    "photoState": {
      "type": "number",
      "minimum": 0,
      "maximum": 3
    },
    "modMessage": {
      "type": "string"
    },
    "deathCode": {
      "type": "string",
      "pattern": "^[0-9]{4}$"
    },
    "admin": {
      "type": "boolean"
    },
    "_id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "__v": {
      "type": "number"
    }
  }
}
